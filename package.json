{
  "name": "WEEB",
  "version": "0.1.3",
  "description": "WEEB is a Modifed Version of Ben Grunfeld's Expack that allows use of Webpack and Express along with Ejs and Bootstrap.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build:dev": "npm run coverage && rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "build:prod": "npm run coverage && npm run lint:fix && rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-jest": "*",
    "babel-loader": "*",
    "codecov": "^3.6.5",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-standard": "*",
    "eslint-loader": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "file-loader": "*",
    "fs": "0.0.1-security",
    "html-loader": "*",
    "html-webpack-plugin": "*",
    "jest": "*",
    "mini-css-extract-plugin": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss-loader": "*",
    "raw-loader": "*",
    "style-loader": "*",
    "uglifyjs-webpack-plugin": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-middleware": "*",
    "webpack-hot-middleware": "*",
    "webpack-node-externals": "*"
  }
}
